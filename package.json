{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "MERN E-Commerce",
  "main": "server.ts",
  "scripts": {
    "start": "npx ts-node backend/server.ts",
    "server": "./node_modules/.bin/nodemon",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "npx ts-node backend/seeder.ts",
    "data:destroy": "npx ts-node backend/seeder.ts -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RdotSilva/TypeScript-MERN-Ecommerce.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RdotSilva/TypeScript-MERN-Ecommerce/issues"
  },
  "homepage": "https://github.com/RdotSilva/TypeScript-MERN-Ecommerce#readme",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/mongoose": "^5.10.3",
    "bcryptjs": "^2.4.3",
    "colors.ts": "^1.0.20",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.13"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
